<h2>Je viens en train</h2>

<%= simple_form_for(@train, data: {turbo: false}) do |f| %>
      <%= f.input :nom %>
      <%= f.input :ville_depart %>
      <%= f.input :ville_arrivee %>
      <%= f.input :date_aller %>
      <%= f.input :heure_aller %>
      <%= f.input :date_retour %>
      <%= f.input :heure_retour %>
      <%= f.input :note %>
      <%= f.button :submit%>
    <% end %>
